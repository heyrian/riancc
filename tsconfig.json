{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"strictNullChecks": true,
		"baseUrl": "src",
		"paths": {
			"@lib/*": ["lib/*"],
			"@utils/*": ["utils/*"],
			"@components/*": ["components/*"],
			"@layouts/*": ["layouts/*"],
			"@assets/*": ["assets/*"],
			"@pages/*": ["pages/*"],
			"@config/*": ["config/*"],
			"@styles/*": ["styles/*"],
		},
		"types": ["vite-plugin-pwa/info"]
	}
}
